{
    "buildCommand": "flutter/bin/flutter build web --release --web-renderer html",
    "installCommand": "git clone https://github.com/flutter/flutter.git -b stable --depth 1",
    "outputDirectory": "build/web",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}